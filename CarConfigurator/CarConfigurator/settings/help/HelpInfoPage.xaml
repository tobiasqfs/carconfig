<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CarConfigurator.settings.help.HelpInfoPage">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="labelCarConfigurator"
                   Margin="15"
                   HorizontalOptions="Start"/>
            <Label x:Name="labelCopyright"
                   Margin="15"
                   HorizontalOptions="Start"/>
            <Button x:Name="OkButton"
                    Text="Ok"
                    Clicked="OkButton_Clicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>